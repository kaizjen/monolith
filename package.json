{
  "name": "monolith",
  "version": "0.0.1",
  "description": "An Electron-based web browser",
  "main": "src/js/main.js",
  "scripts": {
    "start": "electron .",
    "chr:build": "node svlc/svlc.js -r src/browser",
    "chr:start": "node svlc/svlc.js -r src/browser && npm start",
    "ts:build": "tsc",
    "ts:start": "tsc && npm start",
    "mth:build": "node svlc/svlc.js -r src/browser/mth/$svelte",
    "mth:start": "node svlc/svlc.js -r src/browser/mth/$svelte && npm start",
    "build": "npm run ts:build && chr:build && mth:build",
    "dev": "npm run build && electron .",
    "dev-inspect": "npm run build && electron . --inspect",
    "localize": "node src/i18n/transform.js",
    "begin": "npm install && node svlc/hack_empty_svelte_styles.js && npm run build && npm localize"
  },
  "private": true,
  "author": "wheezard",
  "dependencies": {
    "argv-parse": "^1.0.1",
    "electron": "^18.1.0",
    "electron-fetch": "^1.7.4",
    "fs-extra": "^10.0.1",
    "fuse.js": "^6.6.1",
    "i18next": "^21.8.3",
    "runtype-check": "^1.0.3",
    "svelte": "^3.46.4",
    "yaml": "^1.10.2"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^9.0.13"
  }
}
